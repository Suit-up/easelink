{
  "name": "Ease Link",
  "version": "$VER$",
  "manifest_version": 2,
  "description": "__MSG_extension_description__",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "default_locale": "en_US",
  "minimum_chrome_version": "6",
  "background": {
    "scripts": [
      "PrefBranch.js",
      "extension.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [{
    "matches": ["*://*/*"],
    "css": [],
    "js": ["contentscript.js"],
    "all_frames": true
  }]
}
